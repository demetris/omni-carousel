{
  "name": "omni-carousel",
  "type": "module",
  "version": "0.2.4",
  "private": true,
  "description": "A module that adds caroulel-like ehancements to scrollable areas",
  "license": "MIT",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build && pagefind --site dist",
    "build-search-index-for-dev": "astro build && pagefind --site dist && rm -rf public/pagefind && cp -r dist/pagefind public/",
    "preview": "astro preview",
    "astro": "astro",
    "test": "npm run test:run",
    "test:run": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "prepare": "husky"
  },
  "dependencies": {
    "@pagefind/default-ui": "^1.3.0",
    "astro": "^5.11.0",
    "nanoevents": "^9.1.0"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@biomejs/biome": "2.0.6",
    "@csstools/postcss-exponential-functions": "^2.0.9",
    "@eslint/js": "^9.30.1",
    "@types/node": "^24.0.10",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.30.1",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.2",
    "pagefind": "^1.3.0",
    "postcss-logical": "^8.1.0",
    "sass": "^1.89.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1",
    "unist-util-visit": "^5.0.0",
    "vitest": "^3.2.4"
  }
}
